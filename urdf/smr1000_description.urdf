<gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">

      <rosDebugLevel>1</rosDebugLevel>

      <!-- Plugin update rate in Hz -->
      <updateRate>40</updateRate>

      <publishTf>true</publishTf>

      <!-- Name of left joint, defaults to `left_joint` -->
      <leftJoint>wheel_left_joint</leftJoint>

      <!-- Name of right joint, defaults to `right_joint` -->
      <rightJoint>wheel_right_joint</rightJoint>

      <!-- The distance from the center of one wheel to the other, in meters, defaults to 0.34 m -->
      <wheelSeparation>0.816.8</wheelSeparation>

      <!-- Diameter of the wheels, in meters, defaults to 0.15 m -->
      <wheelDiameter>0.1996</wheelDiameter>

      <!-- Wheel acceleration, in rad/s^2, defaults to 0.0 rad/s^2 -->
      <wheelAcceleration>1.0</wheelAcceleration>

      <!-- Maximum torque which the wheels can produce, in Nm, defaults to 5 Nm -->
      <wheelTorque>500.0</wheelTorque>

      <!-- Topic to receive geometry_msgs/Twist message commands, defaults to `cmd_vel` -->
      <commandTopic>cmd_vel</commandTopic>

      <!-- Topic to publish nav_msgs/Odometry messages, defaults to `odom` -->
      <odometryTopic>odom</odometryTopic>

      <!-- Odometry frame, defaults to `odom` -->
      <odometryFrame>odom</odometryFrame>

      <publishOdomTF>true</publishOdomTF>

      <odometrySource>world</odometrySource>

      <!-- Robot frame to calculate odometry from, defaults to `base_footprint` -->
      <robotBaseFrame>base_footprint</robotBaseFrame>

      <!-- Odometry source, 0 for ENCODER, 1 for WORLD, defaults to WORLD -->
      <odometrySource>world</odometrySource>

      <!-- Set to true to publish transforms for the wheel links, defaults to false -->
      <publishWheelTF>false</publishWheelTF>

      <!-- Set to true to publish transforms for the odometry, defaults to true -->
      <publishOdom>true</publishOdom>

      <!-- Set to true to publish sensor_msgs/JointState on /joint_states for the wheel joints, defaults to false -->
      <publishWheelJointState>true</publishWheelJointState>

      <!-- Set to true to swap right and left wheels, defaults to true -->
      <legacyMode>false</legacyMode>
    </plugin>
  </gazebo>

  <link name="lidar_merged_link"/>

  <joint name="base_link_to_lidar_merged_link" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_merged_link"/>
    <origin xyz="0.0 0.0 0.1198" rpy="0 0 0"/>
  </joint>     

  <!-- Front LiDAR SICK Microscan MICS3-AAAZ55AZ1P01 -->
  <gazebo reference="lidar_front_left_link">
    <sensor type="gpu_ray" name="lidar_front_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>923</samples>        <!-- 360 / 0.39 -->
            <resolution>0.39</resolution> <!-- 0.39 deg = 0.0068067840828 rad -->
            <min_angle>-2.3</min_angle>
            <max_angle>2.3</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.05</min>
          <max>30.0</max> <!-- white object -->
          <!-- <max>10.0</max> --> <!-- black object -->
          <resolution>0.05</resolution> <!-- 0.03, 0.04, 0.05, 0.07, 0.15, 0.20 [mm] -->
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_lidar_front_controller" filename="libgazebo_ros_gpu_laser.so">
        <topicName>scan_front</topicName>
        <frameName>lidar_front_left_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

